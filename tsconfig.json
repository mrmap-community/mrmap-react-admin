{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src/**/*",
    "__tests__/**/*"
, "src/OwsContext/tests/.spec.ts", "src/XMLParser/tests/.spec.js"  ],
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "scripts",
    "src/vite-env.d.ts",
    "./vite.config.ts",
    "__tests__"
  ]
}